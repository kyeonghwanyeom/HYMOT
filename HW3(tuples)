#Quiz on Tuples

genres_tuple = ("pop", "rock", "soul", "hard rock", "soft rock", \
                "R&B", "progressive rock", "disco") 
genres_tuple

len(genres_tuple)
# 결과
# 8 

genres_tuple[3]
# 결과
# hard rock

genres_tuple[3:6]
# 결과
#'hard rock', 'soft rock', 'R&B'

genres_tuple[0:2]
# 결과
# 'pop', 'rock'

genres_tuple.index("disco")
# 결과
# 7

C_tuple=(-5, 1, -3)
C_list=sorted(C_tuple)
C_list
# 결과
# [-5, -3, 1]

